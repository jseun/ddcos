#!/bin/bash

source /scripts/config

ROOTFS=${DATADIR}/rootfs
ISODIR=${DATADIR}/iso
LIVEROOTFS=${DATADIR}/live-rootfs
LIVEISOFILE=${DATADIR}/${DISTVER}-live.iso
INSTALLROOTFS=${DATADIR}/install-rootfs
INSTALLISOFILE=${DATADIR}/${DISTVER}-install.iso
export ROOTFS ISODIR LIVEISOFILE INSTALLISOFILE \
  LIVEROOTFS INSTALLROOTFS
